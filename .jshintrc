	"indent"      : 4,
	"bitwise"     : true,
	"camelcase"   : true,
	"curly"       : true,
	"eqeqeq"      : true,
	"forin"       : true,
	"freeze"      : true,
	"immed"       : true,
	"latedef"     : "nofunc",
	"newcap"      : true,
	"noarg"       : true,
	"noempty"     : true,
	"nonew"       : true,
	"undef"       : true,
	"unused"      : true,
	"globalstrict": true,
